{
    "name": "drui628/whitecap",
    "type": "silex-application",
    "description": "A continuous integration server -- based on sismo -- with added functionality including IRC functionality.",
    "keywords": ["sismo", "continuous integration", "irc", "bot"],
    "license": "MIT",
    "authors": [
        {
            "name": "Micah Breedlove",
            "email": "druid628@gmail.com",
            "homepage": "http://micah.breedlove.in/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "silex/silex": ">=1.0.0-dev",
        "sismo/sismo": "dev-master",
        "epochblue/philip": "dev-master",
        "epochblue/philip-plugins": "dev-master",
        "symfony/process": "2.1.*",
        "symfony/event-dispatcher": "2.1.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
      "psr-0": {
        "druid628": "src/"
      }
    }
}
